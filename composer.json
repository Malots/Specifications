{
    "name": "malots/specifications",
    "description": "Specification pattern composer package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "leovene",
            "email": "leo.ochoalopes@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
      "php": "^7.0",
      "phpunit/phpunit": "^6.3.1"
    },
    "autoload": {
      "psr-4": {
        "Malots\\Specifications\\": "src/Specifications",
        "Malots\\SpecificationsTest\\": "tests/SpecificationsTest"
      }
    },
    "config": {
      "preferred-install": "dist",
      "optimize-autoloader": true,
      "process-timeout": 30
    }
}
